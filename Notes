public static void numBlacks(x, y) {										//define function//
	for (int i = 0, i <= word.length - 1, i++) {								//for loop//
		if (y.substring(i) == x.substring(i)) {
			blacks += 1;
		}
	}
	return blacks;


==================================================================================================================================


public static void numWhites(x, y) {										//define function//
	for (int i = 0, i <= word.length - 1, i++) {								//for loop//
		for (int j = 0, j <= word.length - 1, j++) {							//for loop//
			if (y.substring(i) != x.substring(i) && y.substring(i) == x.substring(j) && i != j) {	//ERROR TEST THIS//
				whites += 1;
				x.substring(j) = NULL;								//java equivalent//			
			}
		}
	}
	return whites;


