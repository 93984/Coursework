public static void score(total) {                                                   				//define function//
    int maxScore = 750;
    int maxSeconds = 3*60;
    int x = 0;
    while true {
        x += 0.02;
        time.sleep(0.02);                                                           				//java equivalent//
        if (submit button onclick) {                                                				//java equivalent//
            if (x > 180) {
                return total + 1;
            } else {
                points = int(maxScore*(maxScore^(-x/maxSeconds)))                   				//powers?//
                return total + points;
        }
    }
}
int total = 0;
System.out.println(score(total));
total += score(total);


