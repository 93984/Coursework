public static void end(total, word, attempts) {                                    				//define function//
	if (attempts >= 16) {
		System.out.println(“Sorry, you did not guess ” word + “ in enough tries”);
	} else {
		int maxScore = 750;
        int maxAttempts = 16
		total += maxScore*(maxAttempts-attempts+1);
		System.out.println(“Well done, you guessed ” word + “ in ” + attempts + “ tries”);
		System.out.println(“Total points: ” + points);
		for (int i = 0, i <= 9, i++) {                                              			//for loop//
			if (total > LEADERBOARDS[i]) {                                          		//java equivalent//
				UPDATE LEADERBOARDS;                                                		//java equivalent//
				REMOVE LEADERBOARDS[9]                                              		//java equivalent//
				System.out.println(“You are number ” i+1 + “ on the leaderboard”);
            }
        }
    }
}
int total = 0;
String word = "life";
int attempts = 12;
